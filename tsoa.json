{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*Controller.ts"
  ],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/",
     "entryFile": "./src/app.ts",
    "routesDir": "./src/generated"
  },
  "swagger": {
    "outputDirectory": ".",
    "entryFile": "./src/app.ts"
  }
}